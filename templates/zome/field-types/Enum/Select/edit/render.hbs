<sl-select name="{{snake_case field_name}}" .helpText=${msg("{{label}}{{#if required}}*{{/if}}")} {{#if required}}required{{/if}} {{#if edit}}.defaultValue=${ {{default_value}} }{{/if}}>
{{#each field_type.variants}}
  <sl-option value="{{pascal_case this}}">${msg("{{title_case this}}")}</sl-option>
{{/each}}
</sl-select>
