{
  "name": "@holochain-open-dev/{{kebab_case app_name}}",
  "version": "0.1.0",
  "scripts": {
    "start": "vite --clearScreen false --port $UI_PORT",
    "build": "pnpm lint && tsc && pnpm analyze",
    "lint": "eslint src && prettier \"src/**/*.ts\" --check --ignore-path .gitignore",
    "format": "eslint src --fix && prettier \"src/**/*.ts\" --write --ignore-path .gitignore",
    "localize:extract": "lit-localize extract",
    "localize:build": "lit-localize build",
    "analyze": "cem analyze --litelement --exclude dist",
    "prepare": "tsc && pnpm localize:build && pnpm analyze"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist", "locales", "src"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holochain-open-dev/{{kebab_case app_name}}.git"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dist/*": "./dist/*",
    "./locales/*": "./locales/*"
  },
  "dependencies": {
    "@holochain/client": "^0.17.0-dev.9",
    "@holochain-open-dev/elements": "^0.300.0-dev.0",
    "@holochain-open-dev/stores": "^0.300.0-dev.1",
    "@holochain-open-dev/utils": "^0.300.0-dev.0",
    "@lit/context": "^1.0.0",
    "@lit/localize": "^0.12.0",
    "@mdi/js": "^7.2.0",
    "@msgpack/msgpack": "^2.8.0",
    "@shoelace-style/shoelace": "^2.11.0",
    "lit": "^3.0.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.9.4",
    "@lit/localize-tools": "^0.6.3",
    "@eslint/js": "^9.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.0",
    "tslib": "^2.6.2",
    "vite": "^4.0.0",
    "vite-plugin-checker": "^0.5.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "customElements": "custom-elements.json",
  "type": "module"
}
